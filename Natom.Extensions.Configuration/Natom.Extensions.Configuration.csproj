<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>German Bertolini</Authors>
    <Company>Natom</Company>
    <Product>Natom.Extensions.Configuration</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>2022</Copyright>
    <PackageProjectUrl>https://natom.com.ar/</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <Version>1.0.1</Version>
    <Description>Servicio singleton para proveer la configuración de la aplicación</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.90" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="5.0.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
